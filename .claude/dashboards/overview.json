{
  "dashboard": {
    "title": "Golden Armada - Agent Orchestration Overview",
    "description": "Comprehensive monitoring dashboard for agent execution, performance, and system health",
    "tags": ["golden-armada", "agents", "orchestration", "telemetry"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "templating": {
      "list": [
        {
          "name": "agent_type",
          "type": "query",
          "label": "Agent Type",
          "query": "label_values(agent_execution_total, agent_type)",
          "multi": true,
          "includeAll": true,
          "current": {
            "text": "All",
            "value": "$__all"
          }
        },
        {
          "name": "interval",
          "type": "interval",
          "label": "Interval",
          "query": "1m,5m,10m,30m,1h",
          "current": {
            "text": "5m",
            "value": "5m"
          }
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Agent Execution Rate",
        "type": "graph",
        "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "rate(agent_execution_total{agent_type=~\"$agent_type\"}[$interval])",
            "legendFormat": "{{agent_type}} - {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Executions per second",
            "format": "short"
          }
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "nullPointMode": "null as zero"
      },
      {
        "id": 2,
        "title": "Active Agents",
        "type": "stat",
        "gridPos": { "x": 12, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "count(agent_execution_total)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 1, "color": "yellow" },
                { "value": 5, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "stat",
        "gridPos": { "x": 18, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "sum(rate(agent_execution_total{status=\"failure\"}[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 0.01, "color": "yellow" },
                { "value": 0.1, "color": "red" }
              ]
            },
            "unit": "errors/s"
          }
        }
      },
      {
        "id": 4,
        "title": "Total Tokens Used",
        "type": "stat",
        "gridPos": { "x": 12, "y": 4, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "sum(agent_tokens_used)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 5,
        "title": "Context Window Utilization",
        "type": "gauge",
        "gridPos": { "x": 18, "y": 4, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "avg(context_window_utilization)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "max": 100,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 75, "color": "yellow" },
                { "value": 90, "color": "red" }
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 6,
        "title": "Agent Execution Duration (p95)",
        "type": "graph",
        "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(agent_execution_duration_seconds_bucket{agent_type=~\"$agent_type\"}[$interval])) by (le, agent_type))",
            "legendFormat": "{{agent_type}} p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(agent_execution_duration_seconds_bucket{agent_type=~\"$agent_type\"}[$interval])) by (le, agent_type))",
            "legendFormat": "{{agent_type}} p50",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "label": "Duration (seconds)",
            "format": "s"
          }
        ],
        "lines": true,
        "fill": 0,
        "linewidth": 2
      },
      {
        "id": 7,
        "title": "Tool Call Success Rate",
        "type": "graph",
        "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(rate(tool_call_success_total[$interval])) by (tool) / sum(rate(tool_calls_total[$interval])) by (tool) * 100",
            "legendFormat": "{{tool}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Success Rate (%)",
            "format": "percent",
            "max": 100,
            "min": 0
          }
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2
      },
      {
        "id": 8,
        "title": "Top Tools by Usage",
        "type": "table",
        "gridPos": { "x": 0, "y": 16, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "topk(10, sum(rate(tool_calls_total[5m])) by (tool))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "tool": "Tool",
                "Value": "Calls/sec"
              }
            }
          }
        ]
      },
      {
        "id": 9,
        "title": "Task Queue Size",
        "type": "graph",
        "gridPos": { "x": 12, "y": 16, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "task_queue_size",
            "legendFormat": "Priority {{priority}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Queue Size",
            "format": "short"
          }
        ],
        "lines": true,
        "bars": false,
        "fill": 1,
        "linewidth": 2,
        "stack": true
      },
      {
        "id": 10,
        "title": "Error Breakdown by Type",
        "type": "piechart",
        "gridPos": { "x": 0, "y": 24, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(error_total) by (error_type)",
            "format": "time_series",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          }
        }
      },
      {
        "id": 11,
        "title": "Agent Execution Timeline (Traces)",
        "type": "graph",
        "gridPos": { "x": 12, "y": 24, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "count(telemetry_spans) by (trace_id)",
            "legendFormat": "Trace {{trace_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "label": "Span Count",
            "format": "short"
          }
        ],
        "lines": false,
        "bars": true,
        "fill": 1
      }
    ]
  }
}
